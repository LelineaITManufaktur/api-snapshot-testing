{
    "name": "lelinea/api-snapshot-testing",
    "description": "Provides PHPUnit assertions for api snapshot testing",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "stable",
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "autoload": {
        "psr-4": {
            "Lelinea\\ApiSnapshotTesting\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": { "Tests\\": ["tests/unit"] }
    },
    "require": {
        "php": ">=8.1",
        "ext-json": "*",
        "ext-dom": "*",
        "ext-simplexml": "*",
        "ext-mbstring": "*",
        "phpunit/phpunit": "~9.1",
        "symfony/filesystem": "~2.8|~3.0|~4.0|~5.0",
        "symfony/property-access": "~2.8|~3.0|~4.0|~5.0",
        "webmozart/assert": "~1.2"
    },
    "require-dev": {
        "phpstan/phpstan": "~0.12",
        "phpstan/phpstan-strict-rules": "~0.12",
        "squizlabs/php_codesniffer": "~3.5",
        "slevomat/coding-standard": "~6.0",
        "friendsofphp/php-cs-fixer": "~3.0"
    }
}
